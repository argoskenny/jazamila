{
    "require": {
        "php": "^8.0",
        "laravel/framework": "^12.0",
        "laravel/sanctum": "^4.0",
        "laravel/tinker": "^2.9"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.5",
        "fakerphp/faker": "^1.21",
        "laravel/pint": "^1.13",
        "mockery/mockery": "^1.5",
        "nunomaduro/collision": "^8.6"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "App\\Models\\": "app/Models/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "migrate": [
            "@php artisan migrate"
        ]
    }
}
